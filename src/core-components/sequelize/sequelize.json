{
	"dependencies": [],
	"params": {
		"public": {
			"dialect": {
				"type": "list",
				"options": ["mysql", "mssql", "postgres", "sqlite", "mariadb"]
			}
		},
		"private": {
			"databaseName": {
				"type": "text"
			},
			"username": {
				"type": "text"
			},
			"password": {
				"type": "text"
			},
			"host": {
				"type": "text"
			},
			"port": {
				"type": "number"
			},
			"sync": {
				"type": "boolean",
				"note": "If you want to sync DB with the models folder every time the server starts."
			}
		}
	}
}